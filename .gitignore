.vscode/
.idea/
*.iml

# Python
__pycache__/
*.py[cod]
*$py.class
*.pyo
*.pyd
*.pdb
.build/
build/
dist/
*.egg-info/
.eggs/
.venv/
venv/
env/
ENV/
pip-wheel-metadata/
.pytest_cache/
.coverage
.coverage.*
.cache/
.mypy_cache/
.ruff_cache/
.pytype/
.pyre/
